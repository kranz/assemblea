<%- model_class = Presenza -%>
<div class="page-header">
  <h1><%= t 'recordpresence' %></h1>
</div>
<%= form_for @presenza, :html => { :class => 'form-horizontal' } do |f| %>
  <div class="control-group">
    <%= f.label :delegato, :class => 'control-label' %>
    <div class="controls">
      <%= f.text_field :delegato, 
      				   :data => {:autocomplete_source => '/soci/elenco' } %>
    </div>
  </div>
<% end %>
